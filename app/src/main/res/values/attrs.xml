<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ProgressBarView">
        <attr name="bar_unreach_color" format="color"/>
        <attr name="bar_reach_color" format="color"/>
        <attr name="bar_size" format="dimension"/>
        <attr name="bar_circle_color" format="color"/>
        <attr name="bar_circle_radius" format="dimension"/>
        <attr name="bar_show_hint" format="boolean"/>
        <attr name="bar_disable_color" format="color"/>
        <attr name="android:orientation"/>
        <attr name="android:textColor"/>
        <attr name="android:textSize"/>
    </declare-styleable>
    
    <declare-styleable name="CaseView">
        <attr name="case_background" format="reference"/>
        <attr name="case_background_color" format="color"/>
        <attr name="case_center" format="boolean"/>
        <attr name="case_size" format="dimension"/>
    </declare-styleable>

</resources>